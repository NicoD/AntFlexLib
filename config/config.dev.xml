<?xml version="1.0" encoding="utf-8"?>
<flex-config>
	<compiler>
	    <define>
	        <name>CONFIG::debugging</name>
	        <value>true</value>
	    </define>
	    <define>
	        <name>CONFIG::release</name>
	        <value>false</value>
	    </define>
		
		<allow-source-path-overlap>true</allow-source-path-overlap>
		<debug>true</debug>
		<verbose-stacktraces>true</verbose-stacktraces>
		<omit-trace-statements>false</omit-trace-statements>
		<optimize>false</optimize>
		<warn-constructor-returns-value>true</warn-constructor-returns-value>
		<warn-missing-namespace-decl>false</warn-missing-namespace-decl>
	</compiler>   
	
</flex-config>
